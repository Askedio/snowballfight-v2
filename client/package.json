    "process": "^0.11.10",
    "tailwindcss": "^3.4.17",
    "tile-extruder": "^2.1.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "colyseus.js": "^0.15.9",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "phaser": "^3.87.0",
