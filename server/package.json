{
  "private": true,
  "name": "my-app",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "lib/index.js",
  "scripts": {
    "start": "tsx watch src/index.ts",
    "loadtest": "npx tsx loadtest/example.ts --room default_room --numClients 50 --endpoint ws://localhost:3002",
    "build": "npm run clean && tsc && cd ../client/ && npm i &&  npm run build",
    "clean": "node node_modules/rimraf/bin lib",
    "test": "mocha -r tsx test/**_test.ts --exit --timeout 15000",
    "deploy": "npx @colyseus/cloud deploy"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "^0.15.1",
    "@colyseus/testing": "^0.15.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/matter-js": "^0.19.8",
    "@types/mocha": "^8.2.3",
    "@types/pathfinding": "^0.0.9",
    "copyfiles": "^2.4.1",
    "mocha": "^9.0.2",
    "rimraf": "^2.7.1",
    "tsx": "^3.12.7",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@2toad/profanity": "^3.1.1",
    "@colyseus/command": "^0.3.1",
    "@colyseus/monitor": "^0.15.1",
    "@colyseus/tools": "^0.15.15",
    "@flatten-js/core": "^1.6.2",
    "@kayahr/tiled": "^0.0.1",
    "@types/rbush": "^4.0.0",
    "astar-typescript": "^1.2.7",
    "colyseus": "^0.15.57",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-basic-auth": "^1.2.1",
    "fantasy-name-generator": "^2.0.0",
    "nanoid": "^5.0.9",
    "pathfinding": "^0.4.18",
    "rbush": "^4.0.1"
  }
}
